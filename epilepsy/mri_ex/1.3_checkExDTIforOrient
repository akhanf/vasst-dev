#!/bin/bash

if [ "$#" -lt 2 ]
then
 echo "Usage: $0 <subj> <struct>"
 exit 0
fi
subjid=$1
struct=$2

data_dir=/eq-nas/$USER/EpilepsyDatabase
session=9.4T

dti_dir=$data_dir/$subjid/Ex/$session/${struct}_ScannerOrient
reorient_dir=$data_dir/$subjid/Ex/$session/${struct}

dti=`ls $dti_dir/DTI*.nii.gz | head -n 1`
ref=$reorient_dir/MRI_ExHistReg.nii.gz

echo Comparing $dti and $ref
fslview $dti&
fslview $ref


